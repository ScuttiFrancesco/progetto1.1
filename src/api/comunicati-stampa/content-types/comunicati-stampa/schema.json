{
  "kind": "collectionType",
  "collectionName": "comunicati_stampas",
  "info": {
    "singularName": "comunicati-stampa",
    "pluralName": "comunicati-stampas",
    "displayName": "Comunicati Stampa"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "fonte": {
      "type": "string"
    },
    "comune": {
      "type": "string"
    },
   "contenuto": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "fontePrefisso_da_verificare": {
      "type": "enumeration",
      "enum": ["ciao"]
    },
    "redirectNewPage": {
      "type": "boolean"
    },
    "scadenzaAutomatica": {
      "type": "boolean"
    },
    "thumb": {
      "type": "media",
      "multiple": false,
      "allowedTypes": ["images"]
    },
      "listaLink": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "fotoGallery": {
      "type": "component",
      "component": "immagine.galleria-immagini",
      "repeatable": false
    },

    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": false
    }
  }
}
