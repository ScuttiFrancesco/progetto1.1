{
  "kind": "collectionType",
  "collectionName": "eventis",
  "info": {
    "singularName": "eventi",
    "pluralName": "eventis",
    "displayName": "Eventi"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "titoloNascosto": {
      "type": "string"
    },
    "dataInizio": {
      "type": "string"
    },
    "comune": {
      "type": "string"
    },
    "contenuto": {
      "type": "richtext"
    },
    "InHome_da_verificare": {
      "type": "boolean"
    },
    "redirectUrl": {
      "type": "string"
    },
    "redirectNewPage": {
      "type": "boolean"
    },
    "pulsanteScopri": {
      "type": "boolean"
    },
    "thumb": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "sommario": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "fotoGallery": {
      "type": "component",
      "component": "immagine.galleria-immagini",
      "repeatable": false
    },
    "allegati": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "eventi_da_verificare": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::eventi.eventi"
    },

    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": false
    }
  }
}
